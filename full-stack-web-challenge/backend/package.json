{
  "name": "backend",
  "description": "Backend for the frontend react-vite",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.631.0",
    "@aws-sdk/lib-dynamodb": "^3.631.0",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.7.4",
    "backend": "file:",
    "express": "^4.19.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "aws-sdk-client-mock": "^2.0.0",
    "jest": "^29.2.1"
  },
  "scripts": {
    "create-table": "node src/db/createTable.mjs",
    "check-DynamoDB": "node src/db/checkDynamoDB.mjs",
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/__tests__/**/*.mjs?(x)",
      "**/?(*.)+(spec|test).mjs?(x)"
    ],
    "moduleFileExtensions": [
      "mjs",
      "js"
    ]
  },
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
